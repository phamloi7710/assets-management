{
    "name": "loipham/assets-management",
    "description": "Assets Management For Laravel",
    "license": "MIT",
    "keywords": [

    ],
    "authors": [
        {
            "name": "Loi Pham",
            "email": "phamloi7710@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.6|^7.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LoiPham\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoiPham\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LoiPham\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "LoiPham\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
